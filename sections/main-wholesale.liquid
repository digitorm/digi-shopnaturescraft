        {% if section.settings.enable_1 != blank %}
        <div class="common_box">

        
          {% if section.settings.title_1!= blank %}
          <h5>{{ section.settings.title_1 }}</h5>
          {% endif %}

          {% if section.settings.text_1 != blank %}
          <div class="text">
            {{section.settings.text_1 }}
          </div>
          {% endif %}

          <div class="btn_inn">
              <modal-opener class="product-popup-modal__opener no-js-hidden" data-modal="#PopupModal-healthcare_pro_popup" {{ block.shopify_attributes }}>
                <button id="ProductPopup-healthcare_pro_popup" class="button button--primary" type="button" aria-haspopup="dialog">SELECT AND APPLY</button>
              </modal-opener>
              
            
          
          </div>
        </div>
        {% endif %}

        {% if section.settings.enable_2 != blank %}
        <div class="common_box">

          {% if section.settings.title_2!= blank %}
          <h5>{{ section.settings.title_2 }}</h5>
          {% endif %}

          {% if section.settings.text_2 != blank %}
          <div class="text">
            {{section.settings.text_2 }}
          </div>
          {% endif %}

          <div class="btn_inn">
           <modal-opener class="product-popup-modal__opener no-js-hidden" data-modal="#PopupModal-online_retailerfirst_popup" {{ block.shopify_attributes }}>
                <button id="ProductPopup-online_retailerfirst_popup" class="button button--primary" type="button" aria-haspopup="dialog">SELECT AND APPLY</button>
              </modal-opener>
          
          </div>
        </div>
        {% endif %}

        {% if section.settings.enable_3 != blank %}
        <div class="common_box">

          {% if section.settings.title_3!= blank %}
          <h5>{{ section.settings.title_3 }}</h5>
          {% endif %}

          {% if section.settings.text_3 != blank %}
          <div class="text">
            {{section.settings.text_3 }}
          </div>
          {% endif %}

          
          <div class="btn_inn">
              <modal-opener class="product-popup-modal__opener no-js-hidden" data-modal="#PopupModal-online_retailer_popup" {{ block.shopify_attributes }}>
